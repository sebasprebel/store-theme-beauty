{
  "container-layout#promotions__promotion-1-container":{
    "title":"Contenedor promoción",
    "children":[
      "responsive-image#promotions__promo-1--image",
      "container-layout#promotions-disclosure-1--container"
    ],
    "props":{
      "blockClass":"promotion-container"
    }
  },
  "responsive-image#promotions__promo-1--image":{
    "props":{
      "desktopImage":"/arquivos/banner-promociones-vigentes-desktop2.jpg",
      "mobileImage":"/arquivos/banner-promociones-vigentes-mobile2.jpg",
      "blockClass":"promotion-image"
    }
  },
  "container-layout#promotions-disclosure-1--container":{
    "title":"Contenedor discolusre promoción 1",
    "children":[
      "disclosure-layout#promo-1"
    ],
    "props":{
      "blockClass":"product-page__tab--container-promo"
    }
  },
  "disclosure-layout#promo-1":{
    "title": "Pestaña Promo 1",
    "children": [
      "disclosure-trigger#promo-1", 
      "disclosure-content#promo-1"
    ],
    "props":{
      "initialVisibility":"visible"
    }
  },
  "disclosure-trigger#promo-1":{
    "title": "Título promo 1",
