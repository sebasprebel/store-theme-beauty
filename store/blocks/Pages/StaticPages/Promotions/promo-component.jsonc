      "rich-text#promo-1__title",
      "disclosure-state-indicator"
    ],
    "props":{
      "blockClass":"product-page__tab--trigger"
    }
  },
  "rich-text#promo-1__title": {
    "title":"Título",
    "props": {
      "blockClass": ["tab-title"],
      "font": "t-body",
      "text": "Ver términos y condiciones",
      "textAlignment": "LEFT",
      "textColor": "c-on-base",
      "textPosition": "LEFT"
    }
  },
